#
#  prune2.catt - pruning example
#

import prelude

# This example shows that, at least if we are pruning coherences,
# then disc coherences need special attention....

prune (x : *)
 | id1(x,x,id(x))
 
normalize (x : *)
          (y : *) (f : x -> y)
                  (g : x -> y) (a : f -> g)
          (z : *) (h : y -> z)
                  (k : y -> z) (b : h -> k)
 | h3(x,y,f,g,a,y,id(y),id(y),id1(y,y,id(y)),z,h,k,b)
          